{
    "manifest_version": 3,
    "name": "Sirius自動ログイン",
    "version": "1.0",
    "key":"",
    "permissions": [
        "identity",
        "scripting"
    ],
    "oauth2": {
        "client_id": "~~.apps.googleusercontent.com",
        "scopes": ["https://www.googleapis.com/auth/gmail.readonly"]
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://tuat.ex-tic.com/auth/session"],
            "js": ["session.js"]
        },
        {
            "matches": ["https://tuat.ex-tic.com/auth/session/second_factor"],
            "js": ["second_factor.js"]
        }
    ],
    "host_permissions": ["https://tuat.ex-tic.com/auth/session","https://tuat.ex-tic.com/auth/session/second_factor"]
}
